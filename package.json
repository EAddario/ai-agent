    "typescript": "^5.7.3"
  },
  "dependencies": {
    "got": "^14.4.5",
    "lowdb": "^7.0.1",
    "openai": "^4.83.0",
    "ora": "^8.2.0",
    "terminal-image": "^3.1.1",
    "tsx": "^4.19.2",
    "uuid": "^11.0.5",
    "zod": "^3.24.1",
    "dotenv": "^16.4.7",
    "autoevals": "^0.0.119",
    "@upstash/vector": "^1.2.0",
    "csv-parse": "^5.6.0",
    "run-func": "^3.0.0"
  }
